#Functions
function blissAstraActionBarT2(p: player):
	if {astra.cooldown.%{_p}%} is not true:
		if {ghoost...%{_p}%} is true:
			send action bar "🧊 &bSPOOK  &5🔮   &r🌟 &bTAG" to {_p}
		else:
			send action bar "🔪 &b%{Astra2.%{_p}%}%  &5🔮   &f👻 &b%{Astra3.%{_p}%}%" to {_p}
	if {astra.cooldown.%{_p}%} is true:
		if {ghoost...%{_p}%} is true:
			send action bar "🧊 &bSPOOK  &5🔮 %{entity.%{_p}%}%  &r🌟 &bTAG" to {_p}
		else:
			send action bar "🔪 &b%{Astra2.%{_p}%}%  &5🔮 %{entity.%{_p}%}%  &f👻 &b%{Astra3.%{_p}%}%" to {_p}
function blissAstraActionBarT1(p: player):
	if {astra.cooldown.%{_p}%} is not true:
		send action bar "&5🔺 &aReady! &f👻 &b%{Astra3.%{_p}%}%" to {_p}
	if {astra.cooldown.%{_p}%} is true:
		send action bar "&5🔺 &b%{entity.%{_p}%}% &f👻 &b%{Astra3.%{_p}%}%" to {_p}
function blissAstraPorjectionOff(p: player):
